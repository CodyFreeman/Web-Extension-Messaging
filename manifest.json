{
  "name": "WebExtensionsMessaging",
  "version": "0.0.1",
  "description": "Simple web extension messaging example",
  "applications": {
    "gecko": {
      "id": "codycodescode@gmail.com"
    }
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "manifest_version": 2
}